<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sbsolutions.whatmeal">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App"
        android:allowBackup="false"
        android:icon="@drawable/web_hi_res_512_cook"
        android:label="@string/app_name"
        android:roundIcon="@drawable/web_hi_res_512_cook_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WhatMeanMain">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddIngredient">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".WhatMeanMain" />
        </activity>
    </application>

</manifest>